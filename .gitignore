.env.local
.env.production
*.pem
*.key
*.crt
secrets/
api_keys.txt

# IDE and OS files
.vscode/
.idea/
*.swp
*.swo
*~
.DS_Store
Thumbs.db

# Backend (Java/Maven)
backend/target/
backend/*.jar
backend/*.war
backend/*.log
backend/.mvn/wrapper/maven-wrapper.jar

# Android (Gradle)
android/.gradle/
android/build/
android/app/build/
android/local.properties
